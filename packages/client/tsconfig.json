{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "./build",
		"composite": true,
		"rootDir": ".",
		"skipLibCheck": true,
		"useDefineForClassFields": false,

		"lib": ["ES2020", "DOM", "DOM.Iterable"],

		"moduleResolution": "bundler",
		"isolatedModules": true,
		"moduleDetection": "force",
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": ["src", "vite.config.ts"],
	"files": []
}
